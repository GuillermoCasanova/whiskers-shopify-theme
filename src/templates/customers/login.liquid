<div class="loginPage-wrapper" data-login>
  <div class="loginPage">
    <div class="loginPage-inner  grid  grid--flexDirectionColumn  med-grid--flexDirectionRow">
     <div class="loginPage-logo">
        <a href="/" itemprop="url" >
        <img src="{{'logo--whiskers--badge.svg' | asset_url}}"
         alt="{{ section.settings.logo.alt | default: shop.name }}"
         itemprop="logo"
         class="headerLogo-inner">
        </a>
      </div>
      <div class="loginPage-close">
        <a href="https://www.whiskerslaces.com">
          
        </a>
      </div>
    
      <div class="loginPage-primaryPanel   [ grid  grid--center  grid--justifyCenter ]"  data-panel>
        <div class="customerLogin is-showing" data-login-form>
          <div class="row">
            <div class="small-12 columns">
              <div class="customerLoginHeader">
                <h1 class="headline  customerLoginHeader-headline">{{ 'customer.login.title' | t }}</h1>
                <p>Sign in to your Whiskers account to check on your orders, manage your information and special goodies.</p>
              </div>
              <div class="customerLoginForm" id="CustomerLoginForm">
                <div class="customerLoginForm-inner">  
                  <h2 class="customerLoginForm-headline">
                    Log In
                  </h2>
                      {% form 'customer_login' %}
                        
                        {% if form.errors %}
                          <div class="form-errors">
                            {{ form.errors | default_errors }}
                              <a href="#recover" id="RecoverPassword">{{ 'customer.login.forgot_password' | t }}</a>
                          </div>
                        {% endif %}
                      
                        <div class="input--secondary  input--medium  input--login" data-input-wrapper>
                          <span class="input-icon">
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 123 123"><title>
                              @
