                     <label for="Email" class="hide-elem">
                       {{ 'customer.register.email' | t }}
                     </label>
                     <input type="email"
                       name="customer[email]"
                       id="Email"
                       class="{% if form.errors contains 'email' %}input-error{% endif %}"
                       value="{% if form.email %}{{ form.email }}{% endif %}"
                       spellcheck="false"
                       autocomplete="off"
                       autocapitalize="off"
                       placeholder="{{ 'customer.register.email' | t }}">
                    </div>
                  </div>
                  <div class="small-12 large-6 columns">
                    <div class="input--secondary  input--medium  input--login" data-input-wrapper>
                      <span class="input-icon">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><title>Lock</title><path d="M437.333 192h-32v-42.667C405.333 66.99 338.344 0 256 0S106.667 66.99 106.667 149.333V192h-32C68.771 192 64 196.771 64 202.667v266.667C64 492.865 83.135 512 106.667 512h298.667C428.865 512 448 492.865 448 469.333V202.667c0-5.896-4.771-10.667-10.667-10.667zM287.938 414.823c.333 3.01-.635 6.031-2.656 8.292-2.021 2.26-4.917 3.552-7.948 3.552h-42.667c-3.031 0-5.927-1.292-7.948-3.552-2.021-2.26-2.99-5.281-2.656-8.292l6.729-60.51c-10.927-7.948-17.458-20.521-17.458-34.313 0-23.531 19.135-42.667 42.667-42.667s42.667 19.135 42.667 42.667c0 13.792-6.531 26.365-17.458 34.313l6.728 60.51zM341.333 192H170.667v-42.667C170.667 102.281 208.948 64 256 64s85.333 38.281 85.333 85.333V192z"/></svg>
                      </span>
                     <label for="CreatePassword" class="label-hidden">
                       {{ 'customer.register.password' | t }}
                     </label>
                     <input type="password"
                       name="customer[password]"
                       id="CreatePassword"
                       class="{% if form.errors contains 'password' %}input-error{% endif %}"
                       placeholder="{{ 'customer.register.password' | t }}">
                    </div>
                  </div>
                </div>
                <div class="row large--smallGutter">
                  <div class="small-12 columns">
                   <button type="submit" data-text="{{ 'customer.register.submit' | t }}" class="button--primary  button--medium  button--expanded">
                    {{ 'customer.register.submit' | t }}
                   </button>
                  </div>
                </div>
                {% endform %}
              </div>
            </div>
          </div>
        </div>
      </div>
 
    
      <div class="loginPage-secondaryPanel  [ grid  grid--center grid--justifyCenter ]" data-panel>
        <div class="panelBg">
          <img src="{{'icon_wallpaper_login_screen.svg' | asset_url}}" alt="">
        </div>
         <div class="loginOption  loginOption--noAccount  is-showing" data-sign-up-cta>
            <h1 class="headline   loginOption-headline">Don't have an account?</h1>
            <p>
              Having an accout allows for faster checkout, easier order tracking and earning points for our loyalty program.
            </p>
            <button class="button--primary  button--expanded  button--medium" data-text="Create an Account"  data-show-register>
              CREATE AN ACCOUNT
            </button>
         </div>
         <div class="loginOption  loginOption--haveAccount  is-hidden" data-login-cta>
            <h1 class="headline   loginOption-headline">Already have  an account?</h1>
            <p>
            We're glad to have you back, come on in. 
            </p>
            <button class="button--primary  button--expanded  button--medium" data-text="Log In" data-show-login>
              Log In
            </button>
         </div>
      </div>

    </div>
  </div>
</div>



